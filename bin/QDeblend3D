#!/usr/bin/env python

from PyQt4.QtGui import QApplication
from QDeblend.gui.mainwindow import MainWindow

__version__ = '0.1.0'

def main():
    import sys
    app = QApplication(sys.argv)
    if len(sys.argv)==2:
        file = sys.argv[1]
    else:
        file=''
    wnd = MainWindow(file=file)
    wnd.show()
    sys.exit(app.exec_())
    
if __name__ == '__main__':
    main()

